{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "roto-pros-web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/c3/c3.min.css",
        "assets/css/graph.css",
        "assets/css/bootstrap.min.css",
        "assets/css/stylesheet.css",
        "assets/css/responsive.css",
        "assets/css/owl.carousel.min.css",
        "assets/css/jquery.mCustomScrollbar.css",
        "assets/css/datepic.css",
        "assets/css/bootstrap-slider.min.css",
        "assets/css/main.css",
        "assets/css/main-responsive.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/c3/c3.min.js",
        "assets/js/jquery.js",
        "../node_modules/moment/min/moment.min.js",
        "assets/js/bootstrap.min.js",
        "assets/js/owl.carousel.min.js",
        "assets/js/jquery.mCustomScrollbar.concat.min.js",
        "assets/js/datepicker.js",
        "assets/js/function.js",
        "assets/js/bootstrap-slider.min.js",
        "assets/js/main.js",

        "../node_modules/datatables.net/js/jquery.dataTables.js",
        "../node_modules/datatables.net-bs/js/dataTables.bootstrap.js",
        "../node_modules/datatables.net-responsive/js/dataTables.responsive.js",
        "assets/newAdmin/js/datetime-moment.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
